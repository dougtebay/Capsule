{
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "globals": {
        "axios": true,
        "Vue": true
    },
    "rules": {
        "import/extensions": 0,
        "import/first": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0
    }
}
